{
    "$schema": ".rnv/schema/rnv.template.json",
    "templateConfig": {
        "includedPaths": [
            "buildHooks",
            "src",
            "index.js",
            "tsconfig.json",
            "Gemfile",
            "metro.config.js",
            ".bundle",
            "react-native.config.js",
            "babel.config.js",
            "next.config.js",
            "next-env.d.ts",
            "typings",
            "webpack.config.js"
        ],
        "packageTemplate": {
            "devDependencies": {
                "typescript": "5.0.4",
                "@types/react": "18.2.52",
                "@types/react-dom": "18.2.18",
                "@types/react-native": "0.72.3",
                "@flexn/typescript-config": "1.0.0",
                "@flexn/assets-renative-outline": "0.3.3",
                "babel-loader": "9.1.3"
            },
            "browserslist": {
