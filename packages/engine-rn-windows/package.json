        "prepublishOnly": "yarn clean && yarn compile:prod",
        "watch": "tsc --watch --preserveWatchOutput"
    },
    "dependencies": {
        "@rnv/sdk-react-native": "1.0.0-canary.1",
        "@xmldom/xmldom": "0.7.7",
        "react-native-windows": "0.72.7"
    },
    "devDependencies": {},
    "peerDependencies": {
        "@react-native-community/cli": "*",
        "@rnv/core": "^1.0.0-canary.1",
        "react-native": "*"
    },
    "private": false,
    "publishConfig": {
        "access": "public"
    }
}
