    "contributors": [],
    "files": [
        "lib",
        "renative.engine.json",
        "supportFiles",
        "templates"
    ],
    "main": "lib/index.js",
    "repository": {
        "type": "git",
        "url": "git://github.com/flexn-io/renative.git"
    },
    "scripts": {
        "build": "yarn clean && yarn compile",
        "clean": "rm -rf ./lib && rm -rf tsconfig.tsbuildinfo",
