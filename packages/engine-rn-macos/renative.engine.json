{
    "$schema": "../../.rnv/schema/rnv.engine.json",
    "id": "engine-rn-macos",
    "engineExtension": "rnm",
    "overview": "React native based engine with react-native-macos extension which adds support for macOS platform.",
    "plugins": {
        "react": "source:rnv",
        "react-art": "source:rnv",
        "react-dom": "source:rnv",
        "react-native": "source:rnv",
        "react-native-macos": "source:rnv",
        "@react-native-community/cli": "^11.3.8"
    },
    "npm": {
        "devDependencies": {}
    },
    "platforms": {
        "macos": {
            "npm": {
                "devDependencies": {}
            }
        }
    }
}
