{
    "$schema": "../../.rnv/schema/rnv.project.json",
    "extendsTemplate": "@rnv/template-starter/renative.json",
    "projectName": "@rnv/app-harness",
    "crypto": {
        "encrypt": {
            "dest": "PROJECT_HOME/secrets/privateConfigs.enc"
        },
        "decrypt": {
            "source": "PROJECT_HOME/secrets/privateConfigs.enc"
        }
    },
    "plugins": {
        "@react-native-community/cli": "^11.3.8"
    }
}
