{
    "$schema": "../../.rnv/schema/rnv.engine.json",
    "id": "engine-rn",
    "engineExtension": "rn",
    "overview": "Default react-native based engine. Runs standard react native apps with native support",
    "plugins": {
        "react": "source:rnv",
        "react-art": "source:rnv",
        "react-dom": "source:rnv",
        "react-native": "source:rnv"
    },
    "npm": {
        "devDependencies": {}
    },
    "platforms": {
        "ios": {
            "engine": "engine-rn",
            "npm": {
                "devDependencies": {}
            }
        },
        "macos": {
            "engine": "engine-rn",
            "npm": {
                "devDependencies": {}
            }
        },
        "android": {
            "engine": "engine-rn",
            "npm": {
                "devDependencies": {
                    "jetifier": "2.0.0"
                }
            }
        },
        "androidwear": {
            "npm": {
                "devDependencies": {
                    "jetifier": "2.0.0"
                }
            }
        },
        "androidtv": {
            "npm": {
                "devDependencies": {
                    "jetifier": "2.0.0"
                }
            }
        },
        "firetv": {
            "npm": {
                "devDependencies": {
                    "jetifier": "2.0.0"
                }
            }
        }
    }
}
