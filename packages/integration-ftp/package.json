{
    "name": "@rnv/integration-ftp",
    "version": "0.2.0-alpha.0",
    "description": "Gives RNV the possibility to deploy artifacts to FTP",
    "keywords": [
        "ftp",
        "renative",
        "rnv"
    ],
    "homepage": "https://github.com/flexn-io/renative#readme",
    "bugs": {
        "url": "https://github.com/flexn-io/renative/issues"
    },
    "license": "ISC",
    "author": "Pavel Jacko <pavel.jacko@gmail.com> (https://github.com/pavjacko)",
    "contributors": [
        "Aurimas Mickys <https://github.com/aurimas535>"
    ],
    "main": "lib/index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/flexn-io/renative.git"
    },
    "scripts": {
        "build": "yarn clean && yarn compile",
        "clean": "rm -rf ./lib && rm -rf tsconfig.tsbuildinfo",
        "compile": "tsc -b tsconfig.json",
        "compile:prod": "tsc -b tsconfig.json",
        "prepublishOnly": "yarn clean && yarn compile:prod",
        "watch": "tsc --watch --preserveWatchOutput"
    },
    "dependencies": {
        "dotenv": "8.1.0",
        "ftp-deploy": "2.4.5"
    },
    "devDependencies": {},
    "peerDependencies": {
        "@rnv/core": "^0.38.0-canary.1"
    },
    "publishConfig": {
        "access": "public"
    }
}
